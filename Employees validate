import random as r
class employee():
    def dep1(self):
        for i in range(1,9):
            fd = r.randint(1,5000)
            print("dep1 ID's  : ",fd)
        if(i>=10):
            print("count matched is :",i,": got 10 or more employee! and validated")                
        else:
            print("count matched is :",i,":it's less than 10 employee! can't validate !")
    def dep2(self):
        for i in range(1,50):
            fd = r.randint(1,5000)
            print("dep2 ID's  : ",fd)
        if(i>=10):
            print("count matched is :",i,": got 10 or more employee! and validated")                
        else:
            print("count matched is :",i,":it's less than 10 employee! can't validate !")
    def dep3(self):
        for i in range(1,25):
            fd = r.randint(1,5000)
            print("dep3 ID's  : ",fd)
        if(i>=10):
            print("count matched is :",i,": got 10 or more employee! and validated")                
        else:
            print("count matched is :",i,":it's less than 10 employee! can't validate !")
    def dep4(self):
        for i in range(1,100):
            fd = r.randint(1,5000)
            print("dep4 ID's  : ",fd)
        if(i>=10):
            print("count matched is :",i,": got 10 or more employee! and validated")                
        else:
            print("count matched is :",i,":it's less than 10 employee! can't validate !")
    def dep5(self):
        for i in range(1,5):
            fd = r.randint(1,5000)
            print("dep5 ID's  : ",fd)
        if(i>=10):
            print("count matched is :",i,": got 10 or more employee! and validated")                
        else:
            print("count matched is :",i,":it's less than 10 employee! can't validate !")

            
f= employee()  
f.dep1()
f.dep2()
f.dep3()
f.dep4()
f.dep5()
